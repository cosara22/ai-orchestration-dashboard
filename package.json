{
  "name": "ai-orchestration-dashboard",
  "version": "1.0.0",
  "description": "AI Orchestration Dashboard - Real-time monitoring for Claude Code sessions",
  "private": true,
  "scripts": {
    "dev": "concurrently \"npm run dev:backend\" \"npm run dev:frontend\"",
    "dev:backend": "cd server && bun run dev",
    "dev:frontend": "cd frontend && pnpm dev",
    "build": "npm run build:backend && npm run build:frontend",
    "build:backend": "cd server && bun run build",
    "build:frontend": "cd frontend && pnpm build",
    "test": "bun run tests/integration.test.ts",
    "db:init": "cd server && bun run db:init",
    "docker:up": "docker compose up -d",
    "docker:down": "docker compose down"
  },
  "devDependencies": {
    "concurrently": "^9.0.0"
  }
}
